URxvt.saveLines:         100000
URxvt.scrollBar:         false
URxvt.secondaryScroll:   true
URxvt.font:              -*-tamsyn-medium-*-*-*-20-*-*-*-*-*-*-*
URxvt*italicFont:        -*-tamsyn-medium-*-*-*-20-*-*-*-*-*-*-*
!URxvt.font:xft:IBM Plex Mono:style=Bold:pixelsize=16:antialias=true
!URxvt.font:xft:Cascadia Code:style=Regular:pixelsize=16:antialias=true
URxvt.visualBell:        false
urxvt*secondaryScreen:   false
URxvt*termName:          xterm-256color
URxvt*scrollTtyOutput:   false
URxvt*scrollWithBuffer:  true
URxvt*scrollTtyKeypress: true
URxvt.cursorBlink:       1
URxvt.cursorUnderline:   1
Xft.dpi:                 176

! URxvt.transparent:    True
! URxvt.shading:        20

URxvt*fading:    30
URxvt*fadeColor: #1c1c1c

scratchpad*internalBorder: 30
URxvtFuzzy*internalBorder: 30
URxvtRecord*internalBorder: 30

! hide cursor when not using the mouse
*pointerBlank: true
*pointerBlankDelay: 1


! pencil colors
*foreground: #ABB2BF
*background: #1C1C1C

URxvt.cursorColor: #ABB2BF
